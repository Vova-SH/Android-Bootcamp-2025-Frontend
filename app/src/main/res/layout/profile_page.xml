<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="#253465"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="663dp">

    </FrameLayout>

    <ImageButton
        android:id="@+id/listButton"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="72dp"
        android:layout_marginBottom="13dp"
        android:adjustViewBounds="true"
        android:background="#253465"
        android:cropToPadding="true"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/list2"
        app:tint="#FCF5FD"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck"
        tools:srcCompat="@drawable/list2" />

    <ImageButton
        android:id="@+id/homeButton"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:adjustViewBounds="true"
        android:background="#253465"
        android:cropToPadding="true"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/home"
        app:tint="#FFFFFF"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="670dp"
        tools:srcCompat="@drawable/home" />

    <ImageButton
        android:id="@+id/profileButton"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="13dp"
        android:adjustViewBounds="true"
        android:background="#253465"
        android:cropToPadding="true"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/profile"
        app:tint="#E1A235"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:srcCompat="@drawable/profile" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="237dp"
        android:layout_height="237dp"
        android:layout_marginStart="87dp"
        android:layout_marginTop="78dp"
        android:layout_marginEnd="87dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/profile_photo"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/infoButton"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:adjustViewBounds="true"
        android:background="#FFFFFF"
        android:cropToPadding="false"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/edit"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:srcCompat="@drawable/edit" />

    <ImageView
        android:id="@+id/imagePhone"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="42dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toTopOf="@+id/imageMail"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/phone"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageMail"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="42dp"
        android:layout_marginBottom="17dp"
        app:layout_constraintBottom_toTopOf="@+id/imageGender"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/mail"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageGender"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="42dp"
        android:layout_marginBottom="186dp"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout2"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/gender"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/zen_maru_gothic"
        android:text="@string/phone_number"
        android:textSize="20sp"
        android:typeface="normal"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="87dp"
        tools:layout_editor_absoluteY="350dp" />

    <TextView
        android:id="@+id/textGender"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/zen_maru_gothic"
        android:text="@string/gender"
        android:textSize="20sp"
        android:typeface="normal"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="87dp"
        tools:layout_editor_absoluteY="447dp" />

    <TextView
        android:id="@+id/textMail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/zen_maru_gothic"
        android:text="@string/mail"
        android:textSize="20sp"
        android:typeface="normal"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="87dp"
        tools:layout_editor_absoluteY="400dp" />

</androidx.constraintlayout.widget.ConstraintLayout>